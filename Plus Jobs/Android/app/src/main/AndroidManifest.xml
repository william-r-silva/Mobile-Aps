<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="e.vivi.home">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".EmpresaMenu"
            android:label="@string/title_activity_empresa_menu"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".MainAnonimus"
            android:label="@string/title_activity_main_anonimus"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Cadastro" />
        <activity
            android:name=".MainUser"
            android:label="@string/title_activity_main_user"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>